{
    "name": "pacificdev/laravel_open_ai",
    "description": "A laravel package to interact with the Open AI REST API",
    "license": "MIT",
    "keywords": [
        "laravel",
        "ai",
        "openai"
    ],
    "authors": [
        {
            "name": "Fabio Pacifici",
            "email": "mail@fabiopacifici.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "PacificDev\\LaravelOpenAi\\": "src/",
            "PacificDev\\LaravelOpenAi\\Commands\\": "src/Commands/",
            "PacificDev\\LaravelOpenAi\\Commands\\Presets\\Blade\\": "src/Commands/Presets/Blade",
            "PacificDev\\LaravelOpenAi\\Services\\": "src/Services/",
            "PacificDev\\LaravelOpenAi\\Views\\Components\\": "src/View/Components/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "PacificDev\\LaravelOpenAi\\OpenAiServiceProvider"
            ]
        }
    },
    "require": {
        "pacificdev/laravel_9_preset": "^1.1.1"
    },
    "require-dev": {
        "laravel/breeze": "^1.16",
        "mockery/mockery": "^1.5",
        "pestphp/pest": "^1.23",
        "phpunit/phpunit": "^9.5.10"
    }
}
